<div class="container">
  <div class="row">
    <div class="col s12">
      {{#each articles}}
        <div class="card">
          <div class="card-content">
            <span class="card-title">
              {{title}}
            </span>
            <p>
              {{summary}}
            </p>
            <p><a target="_blank" href={{{link}}}>{{link}}</a></p>
          </div>
          <div class="card-action">
            <a class="waves-effect waves-light btn view-notes" data-id={{{_id}}}>Notes</a>
            <a class="waves-effect waves-light btn delete-article" data-id={{{_id}}}>Delete</a>
          </div>
        </div>
      {{/each}}
    </div>
  </div>
</div>